# Configurazione Fatture in Cloud
# Ottieni questi dati da: https://fattureincloud.it/

# Token di accesso API (OAuth2)
FIC_ACCESS_TOKEN=your_access_token_here

# ID della tua azienda su Fatture in Cloud
FIC_COMPANY_ID=your_company_id_here

# Porta del server backend
PORT=3000

# Configurazione prestazione/servizio standard
SERVICE_NAME=Visita medica
SERVICE_PRICE=50.00
VAT_RATE=0

# Metodo di pagamento predefinito
PAYMENT_METHOD=Contanti

# NOTA: Le prestazioni mediche/sanitarie sono esenti IVA (VAT_RATE=0)
# Art. 10, DPR 633/72
